{
    "name": "spotonlive/sldashboard",
    "description": "Dashboard screen in Laravel 5.1 and NodeJS",
    "keywords": ["dashboard", "laravel", "nodejs"],
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/spotonlive/sldashboard",
    "authors": [
        {
            "name": "Nikolaj LÃ¸venhardt Petersen",
            "email": "np@spotonmarketing.dk"
        },
        {
            "name": "Mark Stourmann",
            "email": "mas@spotonmarketing.dk"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "master-dev"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "xinax/laravel-gettext": "3.*",
        "laravel/socialite": "2.*",
        "webpatser/laravel-uuid": "2.*",
        "knplabs/github-api": "~1.4",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
